package org.porourke.carshop.model;

import java.util.Collection;
import java.util.Set;

import org.porourke.carshop.model.vechicles.Model;

public interface MakeInterface {

	public abstract void addModel(ModelInterface model);

	public abstract long getId();

	public abstract void setId(long id);

	public abstract Collection<ModelInterface> getModels();

	public abstract void setModels(Collection<ModelInterface> models);
	
	public abstract String getName();

	public abstract void setName(String name);

	public abstract int hashCode();

	public abstract boolean equals(Object obj);

	public abstract String toString();

}