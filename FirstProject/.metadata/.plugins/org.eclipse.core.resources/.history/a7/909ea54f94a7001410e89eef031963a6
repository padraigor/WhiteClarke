package org.porourke.carshop.model;

import java.util.Collection;
import java.util.Set;

import org.porourke.carshop.model.vechicles.Make;
import org.porourke.carshop.model.vechicles.Vehicle;

public interface VehiclesInterface {	
	public Collection<Make> getMakes();
	public MakeInterface getMakeById(int id);
	public ModelInterface getModelById(int id);
	public VehicleInterface getVehicleById(int id);
	public void saveVehicle(Vehicle vehicleInterface);
}
