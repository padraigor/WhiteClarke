public interface VehiclesInterface {	
	public Collection<Make> getMakes();
	public MakeInterface getMakeById(long id);
	public ModelInterface getModelById(long id);
	public VehicleInterface getVehicleById(long id);
	public void saveVehicle(Vehicle Vehicle);
}
