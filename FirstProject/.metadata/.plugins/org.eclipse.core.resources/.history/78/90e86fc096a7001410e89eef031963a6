package org.porourke.carshop.model;

import java.util.Collection;
import java.util.Set;

import org.porourke.carshop.model.vechicles.Make;
import org.porourke.carshop.model.vechicles.Vehicle;

public interface ModelInterface {

	public abstract long getId();
	public abstract void setId(long id);
	
	public abstract Collection<VehicleInterface> getVehicles();
	public abstract void setVehicles(Collection<VehicleInterface> vehicles);
	
	public abstract MakeInterface getMake();
	public abstract void setMake(MakeInterface make);
	
	public abstract String getName();
	public abstract void setName(String name);
	
	public abstract int hashCode();
	public abstract boolean equals(Object obj);
	public abstract String toString();

}